      "source": [
        "#Factorial Using Recursion\n",
        "\n",
        "def fact(n):\n",
        "  if (n==0 or n==1):\n",
        "    return 1\n",
        "  else:\n",
        "    return n * fact (n-1)\n",
        "\n",
        "n = int(input(\"The value of n is :- \"))\n",
        "Ans = fact(n)\n",
        "print(f\"The factorial for {n} is {Ans}\")"
      ]
    }
  ]
